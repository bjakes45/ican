<% @members.each do |m| %>
	<div id="content-list " class='pos-votes-card'>
	  <div class="card">
	    <div class="card-block">
	      <h4 class="pos-votes-text">
      		<div class="row">
      			<div class='col-sm-6'>
      				User: <%= m[0].user.name %>
      			</div>
      			<div class='col-sm-6'>
	    			Votes for: <%= m[1] %>
      			</div>
      		</div>
	      </h4>
	      <div class="pos-votes-content">
	    	<h3> <%= m[0].user.name %></h3> 
	        <p>
	        	<%= @position.title %>: <%= @position.description %>
	        </p>
    	    <%= link_to "I'm interested", 
    	    	new_council_position_pos_vote_path(@position.council, @position, vote_id: m[0].user.id),
    	    	class: 'interested' 
    	    %>
	      </div>
	    </div>
	  </div><!-- card -->
	</div>
<% end %>